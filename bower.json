{
  "name": "psoogle",
  "description": "Search engine for PureScript functions",
  "keywords": ["purescript"],
  "ignore": [
    "**/.*",
    "bower_components",
    "node_modules",
    "output",
    "tests",
    "js",
    "tmp",
    "bower.json",
    "Gruntfile.js",
    "package.json"
  ],
  "dependencies": {
    "purescript-arrays"               : "*",
    "purescript-maybe"                : "*",
    "purescript-foldable-traversable" : "*"
  }
}
